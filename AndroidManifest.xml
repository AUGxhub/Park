<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.park"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />
    <!-- 使用到的权限 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".main.WelcomeActivity"
            android:label="@string/title_activity_welcome"
            android:theme="@android:style/Theme.NoTitleBar" >

            <!-- 启动入口 -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fuctions.InfoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.actionbar.LocateActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.actionbar.SearchActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.me.fuctions.SettingActivity"
            android:label="@string/title_activity_setting" >
        </activity>
    </application>

</manifest>